{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["node_modules", "dist"],
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext"],
        "esModuleInterop": true,
        "moduleResolution": "Bundler",
        "isolatedModules": true,
        "incremental": true,
        "baseUrl": ".",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
